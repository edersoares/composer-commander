{
    "name": "dex/composer-commander",
    "description": "Composer Commander",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Eder Soares",
            "email": "edersoares@me.com",
            "homepage": "https://edersoares.me",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dex\\Composer\\Commander\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dex\\Composer\\Commander\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "tests": "Run tests"
    },
    "require": {
        "php": "^8.0",
        "composer-plugin-api": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "composer/composer": "^2.1"
    },
    "extra": {
        "class": "Dex\\Composer\\Commander\\CommanderPlugin"
    }
}
